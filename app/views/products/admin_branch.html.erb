            <img src="/assets/under-heading.png" alt="" />
          </div>
        </div>
      </div>
    </div>
    <!--<h1>Listing Blogs</h1>-->
    <div class="container">
      <% @products.each do |product| %>
          <div class = "row">
            <div class="col-md-4 col-sm-6">
              <div class="blog-post">
                <div class="blog-thumb">
                  <a href="/products/<%= product.id %>">
                    <%= image_tag product.image_url(:thumb).to_s %>
                  </a>
                </div>
                <div class="blog-content">
                  <div class="content-show">
                    <h4><a href="/products/<%= product.id %>">
                      <%= product.name %>
                    </a></h4>
                    <!--<span>17 Aug 2084</span>-->
                  </div>
                  <!--<div class="content-hide">-->
                  <!--<%= product.description %>-->
                </div>
                <!--</div>-->
              </div>
            </div>
            <div class="col-md-8 col-sm-6">
              <div style="font-size:12px;color:#606060 ;text-align:justify">
                <p><%= product.description %></p>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="back" >
              <button type="send"><%= link_to 'Show', product %></button>
              <button type="send"><%= link_to 'Edit', edit_product_path(product) %></button>
              <button type="send"><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></button>
            </div>
          </div>
          <br>
      <% end %>
    </div>
    <br>
    <div class="container">
      <div class="back" >
        <button type="send"><%= link_to 'New Product', new_product_path %></button>
        <button type="send"><%= link_to 'Back', admin_page_welcomes_path %></button>
        <br>
        <br>
      </div>
    </div>


